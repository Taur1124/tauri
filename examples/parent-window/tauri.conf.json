{
  "$schema": "../../tooling/cli/schema.json",
  "build": {
    "distDir": ["index.html"],
    "devPath": ["index.html"],
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "../.icons/32x32.png",
        "../.icons/128x128.png",
        "../.icons/128x128@2x.png",
        "../.icons/icon.icns",
        "../.icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool"
    },
    "allowlist": {},
    "security": {
      "csp": "default-src 'self'"
    },
    "updater": {
      "active": false
    }
  }
}
