{
  "$schema": "../../core/tauri-config-schema/schema.json",
  "build": {
    "distDir": [
      "index.html"
    ],
    "devPath": [
      "index.html"
    ],
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Parent Window",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "../.icons/icon.svg"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool"
    },
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost"
    }
  }
}